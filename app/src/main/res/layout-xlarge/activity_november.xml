<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listen69a"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg_white"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".november">


    <ImageView
        android:id="@+id/picture"
        android:layout_width="550dp"
        android:layout_height="350dp"
        android:layout_gravity="center_vertical"
        android:background="@drawable/november" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignTop="@+id/bottom_n"
        android:background="@drawable/home_btn"
        android:onClick="back" />

    <ImageButton
        android:id="@+id/listen"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_gravity="right"
        android:background="@drawable/listend"
        android:onClick="play" />

    <ImageButton
        android:id="@+id/btn_check"
        android:layout_width="180dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/top_c"
        android:layout_alignRight="@+id/listen"
        android:layout_below="@+id/listen"
        android:layout_marginTop="60dp"
        android:background="@drawable/selector"
        android:clickable="true"
        android:onClick="check" />

    <LinearLayout
        android:id="@+id/bottom_n"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_below="@+id/top_c"
        android:layout_marginTop="25dp"
        android:layout_toLeftOf="@+id/bottom_o"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_o"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="25dp"
        android:layout_toLeftOf="@+id/bottom_v"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_v"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="25dp"
        android:layout_toLeftOf="@+id/bottom_e"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_e"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="25dp"
        android:layout_toLeftOf="@+id/bottom_m"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_m"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="25dp"
        android:layout_toLeftOf="@+id/bottom_b"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_b"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="25dp"
        android:layout_toLeftOf="@+id/bottom_e2"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_e2"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="25dp"
        android:layout_toLeftOf="@+id/bottom_r"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>


    <LinearLayout
        android:id="@+id/bottom_r"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_alignRight="@+id/listen"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="25dp"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/top_c"
        android:layout_width="match_parent"
        android:layout_height="86dp"
        android:layout_alignRight="@+id/listen"
        android:layout_marginTop="430dp"
        android:gravity="right">

        <ImageView
            android:id="@+id/november_r"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:background="@drawable/r_l" />

        <ImageView
            android:id="@+id/november_e"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:background="@drawable/e_l" />

        <ImageView
            android:id="@+id/november_o"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:background="@drawable/o_l" />

        <ImageView
            android:id="@+id/november_e2"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:background="@drawable/e_l" />

        <ImageView
            android:id="@+id/november_b"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:background="@drawable/b_l" />

        <ImageView
            android:id="@+id/november_n"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:background="@drawable/n_l" />

        <ImageView
            android:id="@+id/november_v"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:background="@drawable/v_l" />

        <ImageView
            android:id="@+id/november_m"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:background="@drawable/m_l" />
    </LinearLayout>
</RelativeLayout>
