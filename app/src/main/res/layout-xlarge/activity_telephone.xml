<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listen49"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_pink"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".telephone">

    <ImageView
        android:id="@+id/picture"
        android:layout_width="90dp"
        android:layout_height="180dp"
        android:layout_gravity="center_vertical"
        android:background="@drawable/telephone" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignTop="@+id/bottom_t"
        android:background="@drawable/home_btn"
        android:onClick="back" />

    <ImageButton
        android:id="@+id/listen"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_gravity="right"
        android:background="@drawable/listend"
        android:clickable="true"
        android:onClick="play" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/top_c"
        android:layout_alignRight="@+id/listen"
        android:layout_below="@+id/listen"
        android:layout_marginTop="10dp"
        android:background="@drawable/selector"
        android:clickable="true"
        android:onClick="check" />

    <LinearLayout
        android:id="@+id/top_c"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignRight="@+id/listen"
        android:layout_marginTop="140dp"
        android:gravity="right">

        <ImageView
            android:id="@+id/telephone_e"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/e" />

        <ImageView
            android:id="@+id/telephone_t"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/t" />

        <ImageView
            android:id="@+id/telephone_l"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/l" />

        <ImageView
            android:id="@+id/telephone_e2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/e" />

        <ImageView
            android:id="@+id/telephone_h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/h" />
        <ImageView
            android:id="@+id/telephone_o"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/o" />

        <ImageView
            android:id="@+id/telephone_e3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/e" />
        <ImageView
            android:id="@+id/telephone_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/p" />

        <ImageView
            android:id="@+id/telephone_n"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/n" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_t"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_e"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_e"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_l"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_l"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_e2"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_e2"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_p"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_p"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_h"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_h"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_o"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_o"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_n"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_n"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_e3"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_e3"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_alignRight="@+id/listen"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

</RelativeLayout>
