<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listen46"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_purple"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".printer">

    <ImageView
        android:id="@+id/picture"
        android:layout_width="360dp"
        android:layout_height="360dp"
        android:layout_gravity="center_vertical"
        android:background="@drawable/printer" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignTop="@+id/bottom_p"
        android:background="@drawable/home_btn"
        android:onClick="back" />

    <ImageButton
        android:id="@+id/listen"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_gravity="right"
        android:background="@drawable/listend"
        android:clickable="true"
        android:onClick="play" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/top_c"
        android:layout_alignRight="@+id/listen"
        android:layout_below="@+id/listen"
        android:layout_marginTop="30dp"
        android:background="@drawable/selector"
        android:clickable="true"
        android:onClick="check" />

    <LinearLayout
        android:id="@+id/top_c"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_alignRight="@+id/listen"
        android:layout_marginTop="330dp"
        android:gravity="right">

        <ImageView
            android:id="@+id/printer_r"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:background="@drawable/r" />

        <ImageView
            android:id="@+id/printer_i"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:background="@drawable/i" />

        <ImageView
            android:id="@+id/printer_t"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:background="@drawable/t" />

        <ImageView
            android:id="@+id/printer_e"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:background="@drawable/e" />

        <ImageView
            android:id="@+id/printer_p"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:background="@drawable/p" />

        <ImageView
            android:id="@+id/printer_n"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:background="@drawable/n" />
        <ImageView
            android:id="@+id/printer_r2"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:background="@drawable/r" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_p"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_r"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_r"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_i"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_i"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_n"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_n"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_t"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_t"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_e"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_e"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_r2"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_r2"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_alignRight="@+id/listen"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

</RelativeLayout>
