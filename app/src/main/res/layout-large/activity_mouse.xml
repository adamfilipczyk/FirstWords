<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listen08"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_green"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".mouse">

    <ImageView
        android:id="@+id/picture"
        android:layout_width="280dp"
        android:layout_height="360dp"
        android:layout_gravity="center_vertical"
        android:background="@drawable/mouse" />

    <ImageButton
        android:id="@+id/back"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignTop="@+id/bottom_m"
        android:background="@drawable/home_btn"
        android:onClick="back" />

    <ImageButton
        android:id="@+id/listen"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_gravity="right"
        android:background="@drawable/listend"
        android:onClick="play" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/top_c"
        android:layout_alignRight="@+id/listen"
        android:layout_below="@+id/listen"
        android:layout_marginTop="30dp"
        android:background="@drawable/selector"
        android:clickable="true"
        android:onClick="check" />

    <LinearLayout
        android:id="@+id/top_c"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_alignRight="@+id/listen"
        android:layout_marginTop="330dp"
        android:gravity="right">

        <ImageView
            android:id="@+id/mouse_s"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:adjustViewBounds="false"
            android:background="@drawable/s" />

        <ImageView
            android:id="@+id/mouse_o"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_alignTop="@+id/mouse_s"
            android:layout_toEndOf="@+id/mouse_s"
            android:layout_toRightOf="@+id/mouse_s"
            android:background="@drawable/o" />

        <ImageView
            android:id="@+id/mouse_m"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_alignTop="@+id/mouse_s"
            android:layout_centerHorizontal="true"
            android:layout_toRightOf="@+id/mouse_o"
            android:background="@drawable/m" />

        <ImageView
            android:id="@+id/mouse_e"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_alignTop="@+id/mouse_s"
            android:layout_centerHorizontal="true"
            android:layout_toRightOf="@+id/mouse_m"
            android:background="@drawable/e" />

        <ImageView
            android:id="@+id/mouse_u"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_alignTop="@+id/mouse_s"
            android:layout_centerHorizontal="true"
            android:layout_toRightOf="@+id/mouse_e"
            android:background="@drawable/u" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_m"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_o"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_o"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_u"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_u"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_s"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_s"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_e"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_e"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_alignRight="@+id/listen"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

</RelativeLayout>
