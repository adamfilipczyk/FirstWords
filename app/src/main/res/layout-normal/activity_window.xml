<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listen59c"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_yellow_dark"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".window">

    <ImageView
        android:id="@+id/picture"
        android:layout_width="200dp"
        android:layout_height="230dp"
        android:layout_gravity="center_vertical"
        android:background="@drawable/window" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignTop="@+id/bottom_w"
        android:background="@drawable/home_btn"
        android:onClick="back" />

    <ImageButton
        android:id="@+id/listen"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentEnd="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_gravity="right"
        android:background="@drawable/listend"
        android:clickable="true"
        android:onClick="play" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/top_c"
        android:layout_alignRight="@+id/listen"
        android:layout_below="@+id/listen"
        android:layout_marginTop="20dp"
        android:background="@drawable/selector"
        android:clickable="true"
        android:onClick="check" />

    <LinearLayout
        android:id="@+id/top_c"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignRight="@+id/listen"
        android:layout_marginTop="190dp"
        android:gravity="right">


        <ImageView
            android:id="@+id/window_w"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/w" />

        <ImageView
            android:id="@+id/window_o"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/o" />

        <ImageView
            android:id="@+id/window_d"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/d" />
        <ImageView
            android:id="@+id/window_w2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/w" />
        <ImageView
            android:id="@+id/window_n"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/n" />
        <ImageView
            android:id="@+id/window_i"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/i" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_w"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_i"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>
    <LinearLayout
        android:id="@+id/bottom_i"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_n"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_n"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_d"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_d"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_o"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_o"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/bottom_w2"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_w2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_below="@+id/top_c"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="15dp"
        android:layout_alignRight="@+id/listen"
        android:background="#000000"
        android:orientation="horizontal">

    </LinearLayout>

</RelativeLayout>
